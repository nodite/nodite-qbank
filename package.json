{
  "name": "nodite-qbank",
  "description": "The framework working for track online question banks",
  "version": "0.0.0",
  "author": "Oscaner",
  "bin": {
    "qbank": "./bin/run.js"
  },
  "bugs": "https://github.com/nodite/nodite-qbank/issues",
  "dependencies": {
    "@boterop/convert-excel-to-json": "^2",
    "@inquirer/prompts": "^7",
    "@keyv/postgres": "^2",
    "@langchain/community": "^0",
    "@langchain/core": "^0",
    "@langchain/ollama": "^0",
    "@lesjoursfr/html-to-image": "^1",
    "@nodite/cache-manager-adapter": "github:nodite/type-cacheable-cache-manager-adapter#main",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@type-cacheable/core": "^14",
    "@xenova/transformers": "^2",
    "ansi-colors": "^4",
    "async-mutex": "^0",
    "async-stream-emitter": "^7",
    "axios": "^1",
    "axios-cache-interceptor": "^1",
    "axios-retry": "^4",
    "better-sqlite3": "^11",
    "cache-manager": "^6",
    "cacheable": "^1",
    "chunk": "^0",
    "cli-progress": "^3",
    "crypto-js": "^4",
    "data-uri-to-buffer": "^6",
    "docker-compose": "^1",
    "dotenv": "^16",
    "form-data": "^4",
    "fs-extra": "^11",
    "glob": "^11",
    "html-to-text": "^9",
    "http-cookie-agent": "^6",
    "js-base64": "^3",
    "keyv": "^5",
    "lodash": "^4",
    "md5": "^2",
    "natsort-esm": "^1",
    "node-html-parser": "^7",
    "object-treeify": "^5",
    "pg": "^8",
    "pickleparser": "^0",
    "pkg-dir": "^8",
    "puppeteer": "^24",
    "puppeteer-extra": "^3",
    "puppeteer-extra-plugin-recaptcha": "^3",
    "puppeteer-extra-plugin-stealth": "^2",
    "random-number": "^0",
    "sleep-promise": "^9",
    "streamifier": "^0",
    "tmp": "^0",
    "tough-cookie": "^5",
    "tty-table": "^4",
    "user-agents": "^1",
    "uuid": "^11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19",
    "@commitlint/config-conventional": "^19",
    "@commitlint/types": "^19",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9",
    "@oclif/prettier-config": "^0",
    "@oclif/test": "^4",
    "@types/async-stream-emitter": "^7",
    "@types/better-sqlite3": "^7",
    "@types/boterop__convert-excel-to-json": "npm:@types/convert-excel-to-json@^1.7.4",
    "@types/chai": "^5",
    "@types/chunk": "^0",
    "@types/cli-progress": "^3",
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11",
    "@types/html-to-text": "^9",
    "@types/lodash": "^4",
    "@types/md5": "^2",
    "@types/mocha": "^10",
    "@types/node": "^22",
    "@types/pg": "^8",
    "@types/random-number": "^0",
    "@types/streamifier": "^0",
    "@types/tmp": "^0",
    "@types/user-agents": "^1",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@typescript-eslint/typescript-estree": "^8",
    "chai": "^5",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "eslint-plugin-n": "^17",
    "eslint-plugin-perfectionist": "^4",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-unicorn": "^57",
    "globals": "^16",
    "husky": "^9",
    "mocha": "^11",
    "oclif": "^4",
    "prettier": "^3",
    "shx": "^0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "overrides": {},
  "engines": {
    "node": ">20.6.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nodite/nodite-qbank",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "qbank",
    "dirname": "qbank",
    "commands": "./dist/commands",
    "hooks": {
      "init": [
        "./dist/hooks/init/main"
      ],
      "finally": [
        "./dist/hooks/finally/main"
      ]
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "vendor": {
        "description": "题库供应商，如 “粉笔考研”、“万题库”"
      },
      "bank": {
        "description": "题库，如 “数二”、“英二”"
      },
      "category": {
        "description": "类别，如 “完型填空”"
      },
      "sheet": {
        "description": "章节/篇章/试卷，如 “2022年数学二模”"
      },
      "question": {
        "description": "题目"
      },
      "output": {
        "description": "题库接收方，如 “Anki”、“Notion”"
      }
    }
  },
  "repository": "https://github.com/nodite/nodite-qbank",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . --fix -c eslint.config.mjs",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky"
  },
  "types": "dist/index.d.ts"
}
